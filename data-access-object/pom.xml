	<parent>
		<groupId>com.ramesh.j2ee</groupId>
		<artifactId>j2ee-design-patterns</artifactId>
		<version>0.0.1-SNAPSHOT</version>
	</parent>
	<artifactId>data-access-object</artifactId>

	<dependencies>
